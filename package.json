  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankichi92/react-sat-map.git"
  },
  "bugs": "https://github.com/sankichi92/react-sat-map/issues",
  "type": "module",
  "files": [
    "dist",
    "lib",
    "types/lib",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/react-sat-map.umd.cjs",
  "module": "./dist/react-sat-map.js",
  "types": "./types/lib/main.d.ts",
