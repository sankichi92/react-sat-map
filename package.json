  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:app": "vite build --config vite.config.app.js",
    "fmt": "prettier --write .",
    "lint": "prettier --check ."
  },
  "dependencies": {
    "satellite.js": "^5.0.0"
  },
  "peerDependencies": {
    "maplibre-gl": ">=1.13.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "react-map-gl": ">=7.1.8"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "maplibre-gl": "^4.7.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-map-gl": "^7.1.8",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.0.5"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
