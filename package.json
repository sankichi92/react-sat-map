    "lint": "prettier --check ."
  },
  "dependencies": {
    "satellite.js": "^6.0.0"
  },
  "peerDependencies": {
    "@vis.gl/react-maplibre": ">=8.0.0",
    "maplibre-gl": ">=4.0.0",
